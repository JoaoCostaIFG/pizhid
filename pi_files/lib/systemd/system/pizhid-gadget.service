# put in /lib/systemd/system/
# delete this 3 lines
# enable service after "sudo systemctl daemon-reload"
[Unit]
Description=PIZ_HID usb gadget service
DefaultDependencies=true
Before=network-pre.target
Wants=network-pre.target

[Service]
Type=simple
ExecStart=/usr/bin/pizhid-gadget-init

[Install]
WantedBy=local-fs.target
